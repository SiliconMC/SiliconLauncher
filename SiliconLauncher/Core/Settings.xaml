<adonisControls:AdonisWindow x:Class="SiliconLauncher.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SiliconLauncher"
        mc:Ignorable="d"
        xmlns:adonisUi="clr-namespace:AdonisUI;assembly=AdonisUI"
        xmlns:adonisControls="clr-namespace:AdonisUI.Controls;assembly=AdonisUI"
        xmlns:adonisExtensions="clr-namespace:AdonisUI.Extensions;assembly=AdonisUI"
        Title="Silicon Settings"
        ResizeMode="NoResize" Height="272.34" Width="510.671" Background="#FF121212" TitleBarBackground="#FF121212" WindowStartupLocation="CenterScreen" Closed="AdonisWindow_Closed">
    <Window.Style>
        <Style TargetType="Window" BasedOn="{StaticResource {x:Type Window}}" />
    </Window.Style>
    <Grid Background="#FF121212" Margin="0,0,0,1.518" >
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*" />
            <ColumnDefinition/>
            <ColumnDefinition Width="4*" />
        </Grid.ColumnDefinitions>
        <Label x:Name="SettingsLabel" Content="Settings" HorizontalAlignment="Right" Margin="0,6.78,0,185.042" Width="182" FontFamily="/Silicon;component/Assets/#Inter" FontSize="39" HorizontalContentAlignment="Center"/>
        <Button x:Name="ReturnButton" Content="Return" HorizontalAlignment="Left" Height="20" Margin="176.665,212,0,0" VerticalAlignment="Top" Width="66" FontFamily="/Silicon;component/Assets/#Inter" BorderBrush="#FF242424" Background="#FF202020" Click="ReturnButton_Click" Grid.Column="2" FontSize="10"/>
        <TextBox  Width="65"  VerticalAlignment="Top" HorizontalAlignment="Left" x:Name="MemoryBox" Margin="10,30.78,0,0" Height="26" SelectionBrush="#FF3C3C3C" FontFamily="/Silicon;component/Assets/#Inter" FontSize="11" Background="#FF202020" BorderBrush="#FF242424" Text="4096" IsEnabled="False" Grid.Column="2" />
        <TextBlock x:Name="MemorySuggest" IsHitTestVisible="False" Text="Memory allocated (MB)" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="2.881,10.78,0,0" Padding="6,3,0,0" Height="20" Width="133.75" FontFamily="/Silicon;component/Assets/#Inter" FontSize="11" Foreground="#FFA6A6A6" Grid.Column="2"/>
        <TextBlock x:Name="SiliconVersion" IsHitTestVisible="False" Text="You are using SiliconLauncher v1.0.0D" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="7.252,201.247,34.083,0" Padding="6,3,0,0" Height="20" Width="211" FontFamily="/Silicon;component/Assets/#Inter" FontSize="9" Foreground="#FFA6A6A6" Grid.ColumnSpan="2"/>
        <TextBlock x:Name="Copyright" IsHitTestVisible="False" Text="Developed by SiliconMC" VerticalAlignment="Top" HorizontalAlignment="Center" Margin="7.252,213.967,34.083,0" Padding="6,3,0,0" Height="20" Width="211" FontFamily="/Silicon;component/Assets/#Inter" FontSize="9" Foreground="#FFA6A6A6" Grid.ColumnSpan="2"/>
        <Button x:Name="ReturnButton_Copy" Content="Apply" HorizontalAlignment="Left" Height="20" Margin="108.665,212,0,0" VerticalAlignment="Top" Width="66" FontFamily="/Silicon;component/Assets/#Inter" BorderBrush="#FF242424" Background="#FF202020" Click="ReturnButton_Click" IsEnabled="False" Grid.Column="2" FontSize="10"/>
    </Grid>
    <adonisControls:AdonisWindow.TitleBarContent>
        <StackPanel Orientation="Horizontal"
                    HorizontalAlignment="Right">
            <Button Content="?"
                    Style="{DynamicResource {x:Static adonisUi:Styles.WindowButton}}"
                    FontFamily="/Silicon;component/Assets/#Inter"
                    FontSize="14" />
        </StackPanel>
    </adonisControls:AdonisWindow.TitleBarContent>
</adonisControls:AdonisWindow>